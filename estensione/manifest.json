{
    "manifest_version": 3,
    "name": "ecobyte data counter",
    "version": "1.0",
    "description": "Conta i dati usati dal tuo computer",
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ]
  }
  